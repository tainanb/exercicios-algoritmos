algoritmo "semnome"
// Função :
// Autor :
// Data : 05/05/2021
// Seção de Declarações 
var
cont, S, numero: Inteiro
inicio
cont <- 1
S <- S + numero
Enquanto (cont <= 6) faca
   Escreva("Digite um número: ")
   Leia(numero)
   cont <- cont + 1
   S <- S + numero
FimEnquanto
Escreva(S)
fimalgoritmo
