algoritmo "semnome"
// Função :
// Autor :
// Data : 11/05/2021
// Seção de Declarações 
var
cont, N: Inteiro
inicio
cont <- 1
Escreva("Escolha um número: ")
Leia(N)
      Repita
      EscrevaL(N, " x ", cont, " = ", N * cont)
      cont <- cont + 1
      Ate (cont > 10)
Escreva(" Acabou")
fimalgoritmo
